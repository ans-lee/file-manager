{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.organizeImports": false
  },
  "files.autoSave": "off",
  "eslint.trace.server": "messages",
  "eslint.packageManager": "yarn",
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ]
}